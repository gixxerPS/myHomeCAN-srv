html
  head
    title= title
    script(src='socket.io/socket.io.js')
    script(src='jquery.min.js')
    script(src='script/client.js')
    script(src='debug.js')
  body
    header Alle Botschaften
        h1= message
    p(id="debug") hello debug

  table
    thead
      tr: th OUTPUTS
    tbody
      each out, i in outputs
        tr
          td
            f #{out.disp} 
          td 
            button(onClick="buttonClick(this.id)" type="button" id= out.idON) ON
          td 
            button(onClick="buttonClick(this.id)" type="button" id= out.idOFF) OFF
              
    ul(id="content")
